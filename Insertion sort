#include <stdio.h>
int main()
{ 
 int arr[10] ;
 int i, j,n,tmp;
 printf("Enter total elements: ");
 scanf("%d",&n);
 printf("Enter array elements: ");
 for(i=0; i<n; i++)
 scanf("%d", &arr[i]);
 for (i=0; i <n; ++i)
 {
 printf(" %d", arr[i]);
 } 
 for (i=1 ; i <= n-1; i++) {
 j = i;
 while (j > 0 && arr[j-1] > arr[j]) {
 tmp = arr[j];
 arr[j] = arr[j-1];
 arr[j-1] = tmp;
 j--;
 }
 }
 printf("\n******* Array Sorted in Ascending Order 

*******\n");
 for (i=0; i < n; i++)
 printf(" %d", arr[i]);
 return 0;
